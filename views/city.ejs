  <link rel="stylesheet" href="css/city.css">
  <title>Document</title>
  <script>


    $(document).ready(function () {

        var number1;
        var number2;


      $(".hotels").on("click", function (object) {
        $(".all-images").hide("slow", "swing");
        $(".all-reviews").hide("slow", "swing");
        $(".all-flights").hide("slow", "swing");
        $(".all-hotels").show("slow", "swing");


      });

      $(".hotel-card").on("click", function (object) {
        $(".hotel-card").removeClass("shadow-lg border border-primary");
        $(this).addClass("shadow-lg border border-primary");
  
        $(".my-toast").fadeIn(1000).fadeOut(3000);


        
        if ($(".hotel-card").hasClass("border-primary") && $(".flight-card").hasClass("border-primary")){
        $(".pricebutton").show();
        }


      })

      $(".flight-card").on("click", function (object) {
        $(".flight-card").removeClass("shadow-lg border border-primary");
        $(this).addClass("shadow-lg border border-primary");
        

        $(".flight-toast").fadeIn(1000).fadeOut(3000);

        if ($(".hotel-card").hasClass("border-primary") && $(".flight-card").hasClass("border-primary"))
        {

        $(".pricebutton").show();
        }


      })




      $(".reviews").on("click", function (object) {
        $(".all-images").hide("slow", "swing");
        $(".all-hotels").hide("slow", "swing");
        $(".all-flights").hide("slow", "swing");
        $(".all-reviews").show("slow", "swing");

      });


      $(".images").on("click", function (object) {
        $(".all-hotels").hide("slow", "swing");
        $(".all-reviews").hide("slow", "swing");
        $(".all-flights").hide("slow", "swing");
        $(".all-images").show("slow", "swing");

      });


      $(".flights").on("click", function (object) {
        $(".all-images").hide("slow", "swing");
        $(".all-reviews").hide("slow", "swing");
        $(".all-hotels").hide("slow", "swing");
        $(".all-flights").show("slow", "swing");

      });


      




    });



    $(window).on("scroll", function () {
      if ($(window).scrollTop()) {
        $('nav').addClass('black');
      }

      else {
        $('nav').removeClass('black');
      }
    });







  </script>
</head>

<body>
  <div class="my-nav">
    <nav class="navbar navbar-expand-lg ">
      <a class="navbar-brand" href="#"> TravelIndia.com</a>

      <div class="my-nav-items">

