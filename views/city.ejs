            <li class="nav-item active">
              <a class="nav-link" href="#">Home</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="#">Link</a>
            </li>
            <li class="nav-item">
              <a class="nav-link disabled" href="#">Disabled</a>
            </li>
          </ul>

        </div>
      </div>

    </nav>
  </div>

  <section class="my-header">
    <img class="city-display" src="<%=myobj.bgimageurl%>" alt="">
    <h1 class="header-head">
      <%=myobj.cityname%>
    </h1>
  </section>








  <section id="about-the-city">
    <h1 class="city-heading">About</h1>
    <p>
      <%=myobj.about%>
    </p>
  </section>













<section id="info">
    <button class="hotels btn btn-light shadow-lg p-3 mb-5 bg-white rounded">hotels</button>
    <button class="flights btn btn-light shadow-lg p-3 mb-5 bg-white rounded">flights</button>
    <button class="reviews btn btn-light shadow-lg p-3 mb-5 bg-white rounded">reviews</button>
    <button class="images btn btn-light shadow-lg p-3 mb-5 bg-white rounded">images</button>
    <form action="/pricecal" method="post">
      <button class="images btn btn-light shadow-lg p-3 mb-5 bg-white rounded pricebutton" name="totalprice" type="submit" value="">price</button>
    </form>
    

    <div class="all-hotels">
      <div class="row">
        <%darray.forEach(function(item){%>
          <%if(item.price.strikeOut!=null){%>
            <div class="col-lg-4 col-md-6 detail-col">


              <form action="/hotelprice" method="post">
                <img class="check-image " src="images/check.png">
                <div class="card hotel-card" data-mdb-toggle="modal" data-mdb-target="#exampleModal">
                  <div class="card-header">
                    <img class="details-image" src="<%=item.propertyImage.image.url%>">
                    <h2>
                      <%=item.name%>
                    </h2>
                  </div>
                  <div class="card-body hotel-price">
                    <p>
                      <%=item.price.strikeOut.formatted%> 
                    </p>
                    <button type="submit" class="btn btn-outline-dark" name="price"
                      value="<%=item.price.strikeOut.formatted%>">Select
                      Hotel</button>
                    <br>
                    <br>
              </form>

              <a href="https://www.expedia.co.in/<%=item.name%>.h<%=item.id%>.Hotel-Information"><button
                  class="btn btn-outline-dark" type="button">Check it on web</button></a>
            </div>
      </div>




    </div>

    <%}})%>

      </div>


      </div>


      <div class="position-fixed bottom-0 end-0 p-3 my-toast" style="z-index: 11">

        <div class="toast-body">
          The Hotel has been selected, you can now select the flight.
        </div>
      </div>
      </div>

      <div class="position-fixed bottom-0 end-0 p-3 flight-toast" style="z-index: 11">

        <div class="toast-body">
          The Flight has been selected.
        </div>
      </div>
      </div>






















      <div class="all-flights">
        <%farray.forEach(function(item){%>
          <div class="card flight-card">
            <div class="card-body">
              <h5 class="card-title flight-price">Rupees <%=item.purchaseLinks[0].totalPrice*81%>
              </h5>

              <p class="card-text"><img src="<%=item.segments[0].legs[0].operatingCarrier.logoUrl%>"
                  class="flight-image"> BOM TO DEL</p>
              <p class="card-text"><img src="<%=item.segments[1].legs[0].operatingCarrier.logoUrl%>"
                  class="flight-image"> DEL TO BOM</p>
              <a href="<%=item.purchaseLinks[0].url%>"><button>BOOK NOW</button></a>
              <br>
              <br>
              <form action="/fprice" method="post"><button type="submit" name="price" value="<%=item.purchaseLinks[0].totalPrice*81%>">SELECT</button></form>
          
            </div>
          </div>

          <%})%>
      </div>
































      <div class="all-reviews">


        <h1>Reviews</h1>
        <form action="/review" method="post">
          <div class="form-group">
            <label for="exampleFormControlTextarea1">Post Your review</label>
            <br>
            <br>
            <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" name="review"></textarea>
          </div>
          <button type="submit">Post</button>
        </form>




        <% myobj.review.forEach(function(item){%>
          <div class="card">
            <div class="card-header">
              <h5 class="card-title">
                <%=item.heading%>
              </h5>
            </div>
            <div class="card-body">
              <p class="card-text">
                <%=item.description%>
              </p>
            </div>
          </div>

          <%})%>

      </div>





   <div class="all-images">
        <div class="container">
          <div class="row">
            <div class="col-lg-12 text-center my-2">
